{
  "name": "@myers/cdp-cli",
  "version": "1.0.0",
  "description": "CLI for Chrome DevTools Protocol - optimized for LLM agents with NDJSON output",
  "type": "module",
  "bin": {
    "cdp-cli": "./build/index.js"
  },
  "main": "./build/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "build": "npm run clean && tsc && chmod +x build/index.js",
    "start": "npm run build && node build/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "keywords": [
    "chrome",
    "devtools",
    "cdp",
    "cli",
    "automation",
    "llm",
    "ndjson"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ws": "^8.18.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "@types/ws": "^8.5.13",
    "@types/yargs": "^17.0.33",
    "@vitest/ui": "^4.0.3",
    "c8": "^10.1.3",
    "typescript": "^5.9.2",
    "vitest": "^4.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
